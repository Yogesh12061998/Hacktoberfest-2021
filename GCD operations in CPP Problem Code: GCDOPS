#include <bits/stdc++.h>
using namespace std;

int main() {
    int t;
    cin>>t;
    while(t--){
    int n;
    cin>>n;
    int a[n],sum=0,flag=0;
    for(int i=1;i<=n;i++){
        cin>>a[i];
        if(i%a[i]!=0){
         flag=1;
        }
        
    }
    if(flag==1){
        cout<<"NO"<<"\n";
    }
    else{
        cout<<"YES"<<"\n";
    }
    }
    return 0;
}
